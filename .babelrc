{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    [
      "babel-plugin-module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@": "./",
          "components": "./components",
          "lib": "./lib",
          "api": "./api"
        }
      }
    ]
  ]
}
